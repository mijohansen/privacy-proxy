{
  "version": 2,
  "projects": {
    "browser": {
      "root": "libs/browser",
      "sourceRoot": "libs/browser/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/browser/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/browser"
          ],
          "options": {
            "jestConfig": "libs/browser/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "runtimeChunk": false,
            "vendorChunk": false,
            "optimization": true,
            "outputPath": "dist/libs/browser",
            "tsConfig": "libs/browser/tsconfig.lib.json",
            "packageJson": "libs/browser/package.json",
            "main": "libs/browser/src/index.ts",
            "assets": [
              "libs/browser/*.md"
            ]
          }
        }
      },
      "tags": []
    },
    "demo-ga4": {
      "root": "apps/demo-ga4",
      "sourceRoot": "apps/demo-ga4/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "defaultConfiguration": "production",
          "options": {
            "compiler": "babel",
            "outputPath": "dist/apps/demo-ga4",
            "index": "apps/demo-ga4/src/index.html",
            "baseHref": "/",
            "main": "apps/demo-ga4/src/main.tsx",
            "polyfills": "apps/demo-ga4/src/polyfills.ts",
            "tsConfig": "apps/demo-ga4/tsconfig.app.json",
            "assets": [
              "apps/demo-ga4/src/favicon.ico",
              "apps/demo-ga4/src/assets"
            ],
            "styles": [
              "apps/demo-ga4/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo-ga4/src/environments/environment.ts",
                  "with": "apps/demo-ga4/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "development": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            }
          }
        },
        "serve": {
          "executor": "@nrwl/webpack:dev-server",
          "options": {
            "buildTarget": "demo-ga4:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "demo-ga4:build:production",
              "hmr": false
            },
            "development": {
              "buildTarget": "demo-ga4:build:development",
              "port": 4219
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/demo-ga4/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/demo-ga4"
          ],
          "options": {
            "jestConfig": "apps/demo-ga4/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "privacy-pack": {
      "root": "libs/privacy-pack",
      "sourceRoot": "libs/privacy-pack/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/privacy-pack/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/privacy-pack"
          ],
          "options": {
            "jestConfig": "libs/privacy-pack/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "server": {
      "root": "apps/server",
      "sourceRoot": "apps/server/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/server",
            "main": "apps/server/src/main.ts",
            "tsConfig": "apps/server/tsconfig.app.json",
            "assets": [
              "apps/server/src/assets"
            ],
            "target": "node",
            "compiler": "tsc"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/src/environments/environment.ts",
                  "with": "apps/server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "server:build",
            "port": 4119
          }
        },
        "serveMultiple": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "PORT=4114 nx run server:serve",
              "PORT=4214 nx run server:serve"
            ],
            "parallel": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/server/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/server"
          ],
          "options": {
            "jestConfig": "apps/server/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "soaker": {
      "root": "apps/soaker",
      "sourceRoot": "apps/soaker/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "generatePackageJson": true,
            "outputPath": "dist/apps/soaker",
            "main": "apps/soaker/src/main.ts",
            "tsConfig": "apps/soaker/tsconfig.app.json",
            "assets": [
              "apps/soaker/src/assets"
            ],
            "target": "node",
            "compiler": "tsc"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/soaker/src/environments/environment.ts",
                  "with": "apps/soaker/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "soaker:build",
            "port": 4219
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/soaker/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/soaker"
          ],
          "options": {
            "jestConfig": "apps/soaker/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  },
  "$schema": "./node_modules/nx/schemas/workspace-schema.json"
}
